{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "import-name": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": false,
    "no-var-requires": false,
    "no-console": false
  }
}
